{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": [
      "config",
      "models",
      "routes",
      "server.js",
      "README.md",
      "package.json"
    ],
    "includePattern": ".js$"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "systemName"            : "The H0M3ST3AD Project",
  	"includeDate"           : "true",
  	"navType"               : "inline",
  	"theme"                 : "yeti",
  	"linenums"              : "true",
  	"collapseSymbols"       : "true",
  	"inverseNav"            : "true",
  	"outputSourceFiles"     : "true" ,
  	"outputSourcePath"      : "false",
  	"dateFormat"            : "D/M/YYYY",
  	"syntaxTheme"           : "default",
  	"sort"					        : "true",
  	"search"                : "true"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template"
  }
}
